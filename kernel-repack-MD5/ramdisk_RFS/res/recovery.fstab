# mount     fstype  device                  [device2]   [fstype2]   [fsoptions]         [fsoptions2]

/boot       bml     /dev/block/bml7
/system     rfs     /dev/block/stl9         NULL        NULL         NULL                rw,check=no
/cache      rfs     /dev/block/stl10        NULL        NULL         NULL                rw,check=no
/data       rfs     /dev/block/stl11        NULL        NULL         NULL                rw,check=no
/sdcard     rfs     /dev/block/mmcblk0p1
/sd-ext     ext3    /dev/block/mmcblk0p2    NULL        auto         NULL                rw,nosuid,nodev,check=no
